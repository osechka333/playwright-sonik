{
  "name": "playwright",
  "version": "1.0.0",
  "description": "Playwright testing",
  "main": "index.js",
  "scripts": {
    "test": "npx playwright test",
    "test:main": "npx playwright test --project=main",
    "test:api": "npx playwright test --project=api",
    "test:debug": "npx playwright test --debug",
    "test:positive": "npx playwright test -c ./playwright.config.positive.js",
    "test:negative": "npx playwright test -c ./playwright.config.negative.js",
    "test:ui": "npx playwright test --ui",
    "report": "npx playwright show-report"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "devDependencies": {
    "@playwright/test": "^1.39.0",
    "@types/node": "^20.8.9"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "axios-cookiejar-support": "^4.0.7",
    "dotenv": "^16.3.1",
    "got": "^13.0.0",
    "tough-cookie": "^4.1.3",
    "zod": "^3.22.4"
  }
}
